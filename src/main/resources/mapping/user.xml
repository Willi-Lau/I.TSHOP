        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lwy.demo.mapper.UserMapper">

<!--    执行增加操作-->
    <insert id="adduser" parameterType="com.lwy.demo.bean.User">

         insert into user  (username,password,name,birthday,sex,address,email,grossmoney,alive)
